{
    "name": "statwig/statwig",
    "description": "Static site generator using Twig templating engine",
    "type": "project",
    "require": {
        "twig/twig": "^2.4",
        "symfony/console": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/finder": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Statwig\\Statwig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof",
            "email": "krzysztof@engineer.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    }
}
